.sample-catalog, .sample-tips, .sample-api, .sample-form {
    width: 90%;
}

.sample-tips .warning {
    font-size: 18px;
    color: #FF0000;
}

.sample-tips .f28 {
    font-size: 28px;
}

.sample-tips .f22 {
    font-size: 22px;
}

.sample-tips .txt {
    color: #7e00ff;
    font-size: 20px;
}

.sample-tips .testBtn {
    font-size: 18px;
    color: #FF0000;
}

.sample-api table {
    width: 100%;
    padding: 0;
    border-spacing: 0;
    border-right: 1px solid #57c9e5;
    border-bottom: 1px solid #57c9e5;
}

.sample-api table tbody tr td {
    word-wrap: break-word;
    padding: 5px;
    border-left: 1px solid #57c9e5;
    border-top: 1px solid #57c9e5;
}

.sample-log {
    width: 100%;
    height: 100px;
    padding: 0;
}

